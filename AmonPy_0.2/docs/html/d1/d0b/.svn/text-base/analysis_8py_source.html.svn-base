<div class="line"><a name="l00104"></a><span class="lineno">  104</span>&#160;    <span class="keywordflow">if</span> ev_len==2:</div>
<div class="line"><a name="l00105"></a><span class="lineno">  105</span>&#160;        far_den=evlist[0].false_pos*evlist[1].false_pos*conf.deltaT*fcluster.sigmaQ * \</div>
<div class="line"><a name="l00106"></a><span class="lineno">  106</span>&#160;                conf.cluster_thresh</div>
<div class="line"><a name="l00107"></a><span class="lineno">  107</span>&#160;    <span class="keywordflow">elif</span> ev_len==3:</div>
<div class="line"><a name="l00108"></a><span class="lineno">  108</span>&#160;        far_den=0.5*evlist[0].false_pos*evlist[1].false_pos*evlist[2].false_pos * \</div>
<div class="line"><a name="l00109"></a><span class="lineno">  109</span>&#160;                (conf.deltaT*fcluster.sigmaQ*conf.cluster_thresh)**2</div>
<div class="line"><a name="l00110"></a><span class="lineno">  110</span>&#160;    <span class="keywordflow">else</span>:</div>
<div class="line"><a name="l00111"></a><span class="lineno">  111</span>&#160;        <span class="comment"># generalize for 3+ events</span></div>
<div class="line"><a name="l00112"></a><span class="lineno">  112</span>&#160;        <span class="keywordflow">pass</span> </div>
<div class="line"><a name="l00113"></a><span class="lineno">  113</span>&#160;    <span class="keywordflow">return</span> far_den  </div>
<div class="line"><a name="l00114"></a><span class="lineno">  114</span>&#160;                                               </div>
